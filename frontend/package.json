    "cron-validator": "^1.3.1",
    "dotenv": "^16.4.5",
    "formik": "^2.4.6",
    "framer-motion": "^11.2.10",
    "next-themes": "^0.3.0",
    "react": "^18.2.0",
    "react-ace": "^11.0.1",
    "react-auth-kit": "^3.1.3",
    "react-dom": "^18.2.0",
